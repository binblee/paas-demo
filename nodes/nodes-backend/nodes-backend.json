{
  "kind": "ReplicationController",
  "apiVersion": "v1",
  "metadata": {
    "name": "nodes-backend-rc-v1",
    "labels": {
      "role": "nodes-backend-rc",
      "bizgroup": "nodes-frontend-backend-demo"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "app": "nodes-backend",
      "deployment": "v1",
      "bizgroup": "nodes-frontend-backend-demo"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "nodes-backend",
          "deployment": "v1",
          "bizgroup": "nodes-frontend-backend-demo"
        }
      },
      "spec": {
        "volumes": null,
        "containers": [
          {
            "name": "nodes-backend",
            "image": "binblee/nodes-backend:latest",
            "ports": [
              {
                "containerPort": 8081,
                "protocol": "TCP"
              }
            ],
            "imagePullPolicy": "Always"
          }
        ],
        "restartPolicy": "Always",
        "dnsPolicy": "ClusterFirst"
      }
    }
  }
}

